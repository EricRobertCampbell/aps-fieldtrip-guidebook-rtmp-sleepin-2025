      \field{journaltitle}{Canadian Journal of Earth Sciences}
      \field{month}{8}
      \field{note}{Publisher: {NRC} Research Press}
      \field{number}{8}
      \field{shortjournal}{Can. J. Earth Sci.}
      \field{shorttitle}{Hadrosaurid (Edmontosaurus) bonebeds from the Horseshoe Canyon Formation (Horsethief Member) at Drumheller, Alberta, Canada}
      \field{title}{Hadrosaurid (Edmontosaurus) bonebeds from the Horseshoe Canyon Formation (Horsethief Member) at Drumheller, Alberta, Canada: geology, preliminary taphonomy, and significance}
      \field{urlday}{18}
      \field{urlmonth}{4}
      \field{urlyear}{2024}
