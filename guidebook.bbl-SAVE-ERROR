      \field{journaltitle}{Canadian Journal of Earth Sciences}
      \field{langid}{english}
      \field{month}{7}
      \field{number}{7}
      \field{shortjournal}{Can. J. Earth Sci.}
      \field{shorttitle}{Dinosaur biostratigraphy of the Edmonton Group (Upper Cretaceous), Alberta, Canada}
      \field{title}{Dinosaur biostratigraphy of the Edmonton Group (Upper Cretaceous), Alberta, Canada: evidence for climate influence}
