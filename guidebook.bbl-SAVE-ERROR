      \strng{namehash}{d01d8a9430cbaa536c2c3fee9adeccf9}
      \strng{fullhash}{d01d8a9430cbaa536c2c3fee9adeccf9}
      \strng{bibnamehash}{d01d8a9430cbaa536c2c3fee9adeccf9}
      \strng{authorbibnamehash}{d01d8a9430cbaa536c2c3fee9adeccf9}
      \strng{authornamehash}{d01d8a9430cbaa536c2c3fee9adeccf9}
