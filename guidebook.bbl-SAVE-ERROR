      \strng{namehash}{7d4e5a0a8b61f139c00df0a676b2495b}
      \strng{fullhash}{fcdd5b80daf4a0cb3f5e97985c870860}
      \strng{bibnamehash}{7d4e5a0a8b61f139c00df0a676b2495b}
      \strng{authorbibnamehash}{7d4e5a0a8b61f139c00df0a676b2495b}
